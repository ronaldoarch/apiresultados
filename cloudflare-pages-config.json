{
  "build": {
    "command": "",
    "output": "/"
  },
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/api/*",
      "destination": "https://seu-servidor-php.com/api_resultados.php/:splat",
      "statusCode": 200
    }
  ]
}
